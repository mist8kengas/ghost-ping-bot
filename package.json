  "dependencies": {
    "@discordjs/builders": "^1.2.0",
    "@discordjs/rest": "^1.2.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0"
  },
  "type": "module",
  "devDependencies": {
    "typescript": "^4.5.5"
  }
